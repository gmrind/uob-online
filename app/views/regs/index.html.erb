<p id="notice"><%= notice %></p>

<h1>Listing Regs</h1>

<table>
  <thead>
    <tr>
      <th>Student Name</th>
      <th>Father Name</th>
      <th colspan="3",>Navigation</th>
    </tr>
  </thead>

  <tbody>
    <% @regs.each do |reg| %>
      <tr>
        <td><%= reg.sname %></td>
        <td><%= reg.fname %></td>
        <td align='center'><%= link_to 'Show', reg %></td>
        <td align='center'><%= link_to 'Edit', edit_reg_path(reg) %></td>
        <td><%= link_to 'Destroy', reg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

